let x = 1
let y = 2
let result = x + y

# here's a comment
fn print_result() {
	if result > 0 {
		print(result)
	}
}



# we have maps, and . notation now
let cat = {
	name: "hayley"
	age: 8
}

print(cat.name)

# they also work like dictionaries with any kind of key
let kids = {
	cat = "toothless"
	:me = "xander"
}

print(kids @ cat)
fn main() {
	print(cat.name)
	print(kids@cat)
}